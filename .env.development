# Development environment variables
VITE_NODE_ENV=development
VITE_API_BASE_URL=http://localhost:8000
VITE_SITE_URL=http://localhost:3000

# Development tools
VITE_ENABLE_DEVTOOLS=true
VITE_ENABLE_HOT_RELOAD=true
VITE_ENABLE_SOURCE_MAPS=true

# Analytics (disabled in development)
VITE_GOOGLE_ANALYTICS_ID=
VITE_GOOGLE_TAG_MANAGER_ID=

# Performance monitoring (disabled in development)
VITE_SENTRY_DSN=
VITE_PERFORMANCE_MONITORING=false

# Feature flags
VITE_ENABLE_SERVICE_WORKER=false
VITE_ENABLE_OFFLINE_MODE=false
VITE_ENABLE_PERFORMANCE_MONITORING=false
VITE_ENABLE_ERROR_REPORTING=false

# Build optimizations (disabled for faster development)
VITE_USE_CDN=false
VITE_ENABLE_COMPRESSION=false
VITE_ENABLE_TREE_SHAKING=false
VITE_ENABLE_CODE_SPLITTING=false

# Security (relaxed for development)
VITE_ENABLE_CSP=false
VITE_ENABLE_HSTS=false

# Cache settings (short duration for development)
VITE_CACHE_VERSION=dev
VITE_STATIC_CACHE_DURATION=0
VITE_API_CACHE_DURATION=0
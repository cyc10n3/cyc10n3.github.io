# Production environment variables
VITE_NODE_ENV=production
VITE_API_BASE_URL=https://api.wellheadequipment.com
VITE_SITE_URL=https://wellheadequipment.com
VITE_CDN_URL=https://cdn.wellheadequipment.com

# Analytics and tracking
VITE_GOOGLE_ANALYTICS_ID=GA_MEASUREMENT_ID
VITE_GOOGLE_TAG_MANAGER_ID=GTM_CONTAINER_ID

# Performance monitoring
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
VITE_PERFORMANCE_MONITORING=true

# Feature flags
VITE_ENABLE_SERVICE_WORKER=true
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_ERROR_REPORTING=true

# Build optimizations
VITE_USE_CDN=false
VITE_ENABLE_COMPRESSION=true
VITE_ENABLE_TREE_SHAKING=true
VITE_ENABLE_CODE_SPLITTING=true

# Security
VITE_ENABLE_CSP=true
VITE_ENABLE_HSTS=true

# Cache settings
VITE_CACHE_VERSION=1.0.0
VITE_STATIC_CACHE_DURATION=31536000
VITE_API_CACHE_DURATION=300